#include "MyClass.hxx"
#include "PersistentObjectFactory.hxx"
#include "MyClassImplementation.hxx"

namespace MyClass {

  CLASSNAMEINIT(MyClass);

  /* Default constructor */
  MyClass::MyClass()
    : OT::Base::Common::TypedInterfaceObject<MyClassImplementation>(new MyClassImplementation)
  {
    // Nothing to do
  }

  /* String converter */
  OT::String MyClass::str() const
  {
    return OT::OSS() << "class=" << MyClass::GetClassName()
		     << " implementation=" << getImplementation()->str();
  }


} /* Namespace MyClass */
